    },
    "Model": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Year": {
      "type": "integer",
      "required": true
    },
    "Price": {
      "type": "biginteger",
      "required": true
    },
    "Stock": {
      "type": "string",
      "required": true
    },
    "Seats": {
      "type": "integer",
      "required": true
    },
    "Trunk": {
      "type": "integer",
      "required": true
    },
    "Speed": {
      "type": "integer",
      "required": true
    },
    "Handling": {
      "type": "integer",
